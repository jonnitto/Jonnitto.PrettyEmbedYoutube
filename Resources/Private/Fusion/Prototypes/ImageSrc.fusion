prototype(Jonnitto.PrettyEmbedYoutube:ImageSrc) < prototype(Neos.Fusion:Array) {
    @context.videoID = Jonnitto.PrettyEmbedYoutube:VideoID

    youtubeImage = ${'//img.youtube.com/vi/' + videoID + '/' + configuration.imgResultion + '.jpg'}
    youtubeImage.@if.hasNoPicture = ${q(node).property('customPreviewImage') == null}
    customPreviewImage = Jonnitto.PrettyEmbedYoutube:ImageUri
}
